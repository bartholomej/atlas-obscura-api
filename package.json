{
  "name": "atlas-obscura-api",
  "version": "4.3.0",
  "description": "Atlas Obscura API in JavaScript. NPM library for scrapping atlasobscura.com",
  "author": "BART! <bart@bartweb.cz>",
  "type": "module",
  "scripts": {
    "start": "tsc -w",
    "prebuild": "rimraf dist",
    "build": "tsdown --config-loader unconfig && cp README.md dist/ && cp LICENSE dist/",
    "build:server": "npx esbuild ./server.ts --outfile=dist/server.js --format=esm --sourcemap && node scripts/server-mod.js",
    "server": "yarn tsx server.ts",
    "tsc": "tsc",
    "demo": "tsx demo",
    "lint": "eslint ./src/**/**/* --fix",
    "test": "vitest",
    "test:coverage": "yarn test run --coverage",
    "publish:next": "yarn && yarn build && yarn test --coverage && npm publish --folder dist --tag beta",
    "postversion": "git push && git push --follow-tags",
    "release:beta": "npm version preminor --preid=beta -m \"chore(update): prelease %s Î²\"",
    "prerelease:beta": "npm version prerelease --preid=beta -m \"chore(update): prelease %s Î²\"",
    "release:patch": "git checkout master && npm version patch -m \"chore(update): patch release %s ðŸ›\"",
    "release:minor": "git checkout master && npm version minor -m \"chore(update): release %s ðŸš€\"",
    "release:major": "git checkout master && npm version major -m \"chore(update): major release %s ðŸ’¥\"",
    "prepare": "husky"
  },
  "dependencies": {
    "got-scraping": "^4.1.2",
    "node-html-parser": "^7.0.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.3",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitest/coverage-istanbul": "^4.0.15",
    "@vitest/coverage-v8": "4.0.15",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "express": "^5.2.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "rolldown": "1.0.0-beta.45",
    "tsdown": "^0.17.2",
    "tsx": "^4.21.0",
    "typescript": "^6.0.0-dev.20251210",
    "vitest": "^4.0.15"
  },
  "repository": {
    "url": "git+https://github.com/bartholomej/atlas-obscura-api.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/bartholomej/atlas-obscura-api/issues"
  },
  "homepage": "https://github.com/bartholomej/atlas-obscura-api#readme",
  "keywords": [
    "travellers",
    "atlasobscura",
    "places",
    "maps",
    "nodejs",
    "node",
    "typescript",
    "scraper",
    "parser",
    "api"
  ],
  "engines": {
    "node": ">= 20"
  },
  "license": "MIT",
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "packageManager": "yarn@4.10.3",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  }
}